//console.log(url+'test!!!');

//Vue.prototype.$Api = 'http://192.168.0.100:8421'//(B0009test)
Vue.prototype.$localVideo="http://beefindtech.cn:50009";//获取本地视频服务器的
Vue.prototype.$lookVideo='http://beefindtech.cn:10009/live/';
