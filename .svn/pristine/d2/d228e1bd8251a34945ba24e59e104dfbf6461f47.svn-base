//Video

window.eventbus = new Vue();

if(localStorage.getItem('OnToInNet') === 'true'){
  Vue.prototype.$Api = 'http://192.168.0.100:8421'//(转发全局请求头
}else {
  Vue.prototype.$Api = 'http://beefindtech.cn:40009'//(转发全局请求头
};
eventbus.$on('OnToInNet',function (date) {
  if (date) {
    Vue.prototype.$Api = 'http://192.168.0.100:8421'//(转发全局请求头
  }else{
    Vue.prototype.$Api = 'http://beefindtech.cn:40009'//(转发全局请求头
  }
});

Vue.use(VideoPlayer);
